    <image bindtap="handleProxy" class="_img data-v-92bc3422 music_img" src="{{isMusicPlay ? '/static/images/music/music-start.png' : '/static/images/music/music-stop.png'}}" alt data-eventid="{{'0'}}" data-comkey="{{$k}}"></image>
